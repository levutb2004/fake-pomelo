    "version": "0.2.0",
    "configurations": [
        {
            "name": "RWA superpixel_disagg_model.py",
            "type": "python",
            "request": "launch",
            "program": "superpixel_disagg_model.py",
            "console": "integratedTerminal",
            "env": {
                "PYTHONPATH": "${workspaceRoot}",
                "PYDEVD_WARN_EVALUATION_TIMEOUT": "500"},
            "args": [
                "-train", "rwa",
                "-train_lvl", "c",
                "-test", "rwa",
                "-wr", "0.01",
                "--dropout", "0.4", 
                "-lstep", "800",
                "--validation_fold", "0",
                "-rs", "42",
                "-mm", "m",
                "--loss", "LogL1",
                "--dataset_dir", "datasets", 
                "--sampler", "custom",
                "--max_step", "2000", 
                "--name", "rwa_fine_vfold0"
            ]
        }
    ]
}